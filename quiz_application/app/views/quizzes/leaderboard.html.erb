<table>
    <thead>
        <tr>
            <th>User</th>
            <th>Score</th>
            <th colspan="3"></th>
        </tr>
    </thead>

  <tbody>
        <% @quiz_attempts.each do |quiz_attempt| %>
        <% @user = User.find_by(id: quiz_attempt.user_id)%>
        <tr>
            <td><%= @user.name %></td>
            <td><%= quiz_attempt.score %></td>
        </tr>
        <% end %>
    </tbody>
</table>

<%= link_to("Back", quizzes_path)%>

